{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "./dist/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "./dist/server/entry.mjs"
      }
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  }
}
